---
import '@fontsource/poppins/400.css';
import '@fontsource/poppins/600.css';
import '@fontsource/poppins/800.css';

// Nunito
// Dosis
---

<style is:inline is:global>
  :root {
    --jh-font-sans: 'Poppins';
    --jh-font-serif: var(--jh-font-sans);
    --jh-font-heading: var(--jh-font-sans);

    --jh-color-primary: #e42b4c;
    --jh-color-secondary: #a82234;
    --jh-color-accent: #e42b4c;
    --jh-color-text-page: #14161a;
    --jh-color-text-muted: #4b5563;
    --jh-color-bg-page: #ffffff;
  }
</style>
